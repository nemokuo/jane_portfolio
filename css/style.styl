.search-wrap
	position relative
	input
		height 40px
		line-height 40px
		padding 0 45px 0 15px
		margin 0
		border 1px solid $lgray
	
.search-button
	background none
	border none
	outline none
	height 40px
	width 40px
	font-size 14px
	line-height 40px
	position absolute
	top 0
	right 0
