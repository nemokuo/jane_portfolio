			text-align center

/*----------------------*/ 
/*   26-COMMENTS        */ 
/*----------------------*/

.comments-list,
.comments-list > li > ul
	padding 0
	margin 0
	list-style-type none
	
.comment .depth-2
	padding-left 50px
	
.comment-wrap
	margin-bottom 50px

.comment-wrap	
	.avatar
		margin-right 30px
		&:hover
			opacity .5
		img
			height 60px
			width 60px
			border-radius 50%
			
.comment-meta
	margin-bottom 10px
	.name
		display inline-block
		position relative
		font-weight 600
	.time
		padding-left 15px
		color #777

@media (max-width: 768px)
	.comment .depth-2
		padding-left 0


/*----------------------*/ 
/*   27-SCROLL_TOP      */ 
/*----------------------*/

.scroll-top
	position fixed
	width 40px
	height 40px
	text-align center
	line-height 40px
	right 30px
	bottom 30px
	z-index 16
	font-size 13px
	background-color $black
	color $lwhite
	transition $transition
	&:hover i
		animation animateIconTop .4s forwards
	&:hover, &:focus
		background-color $black
		color $lwhite
	i
		color inherit
		display inline-block
	&-visible
		opacity 1
		transform translateX(0)
	&-hidden
		opacity 0
		transform translateX(20px)

@media (max-width: 468px)
	.scroll-top
		display none

/*----------------------*/ 
/*   28-PRELOADER       */ 
/*----------------------*/

$circleOne = 80px
$circleTwo = 60px

.preloader
	position fixed
	top 50%
	left 50%
	height $circleOne
	width $circleOne
	margin-top -($circleOne/2)
	margin-left -($circleOne/2)
	z-index 9999
	&::before, &::after
		content ''
		display block
		position absolute
		border-radius 50%
		background-color $lwhite
		top 50%
		left 50%
	&::before
		height $circleOne
		width $circleOne
		margin-left -($circleOne/2)
		margin-top -($circleOne/2)
		box-shadow 0px -4px 8px 0px rgba(0, 0, 0, 0.08)
		animation spinTwo 3s infinite	
	&::after
		height $circleTwo
		width $circleTwo
		margin-left -($circleTwo/2)
		margin-top -($circleTwo/2)
		box-shadow 0px -4px 8px 0px rgba(0, 0, 0, 0.08)
		animation spinTwo 5s infinite

@keyframes spinOne
	100%
		transform rotate(360deg)

@keyframes spinTwo
	100%
		transform rotate(-360deg)

/*----------------------*/ 
/*   29-PAGE 404        */ 
/*----------------------*/
 
.error404 .aligner
	min-height 100vh