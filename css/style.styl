/*----------------------*/ 
/*   01-IMPORT FONTS    */ 
/*----------------------*/

@require 'shortcodes.styl'

@import url('https://fonts.googleapis.com/css?family=Sorts+Mill+Goudy')
@import url('https://fonts.googleapis.com/css?family=Source+Sans+Pro')


$f1 = 'Source Sans Pro', sans-serif
$f2 = 'Sorts Mill Goudy', serif
/*----------------------*/ 
/*   02-VARIABLES       */ 
/*----------------------*/

$white = #ffffff
$lwhite = #fcfcfc
$border = #f4f4f4
$black = #222222
$lblack = #2b2b2b
$gray = #666666
$lgray = #efefef
$llgray = #f4f4f4
$text = #555555
$red = #fb3b3b

$transition = all .4s ease-in-out

::-moz-selection
	background $lblack
	color $lwhite

::selection
	background $lblack
	color $lwhite

/*----------------------*/ 
/*   03-GENERAL         */ 
/*----------------------*/
	
body
	background-color $white
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
	position relative
	min-width 320px
	font-family $f1
	line-height 1.8
	font-size 15px
	letter-spacing 0.02em
	color $text

h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6
	padding 0
	margin 0
	margin-bottom 5px
	color $black
	font-weight 400
	line-height 1.7em
	letter-spacing 1px
	text-transform uppercase

h1, .h1
	font-size 44px

h2, .h2
	font-size 36px

h3, .h3
	font-size 28px
	
h4, .h4
	font-size 24px
	
h5, .h5
	font-size 18px
	
h6, .h6
	font-size 15px

img
	max-width 100%
	height auto

img.align-left
	margin 0 20px 20px 0
	
img.align-right
	margin 0 0 20px 20px

p
	margin 0 0 20px 0

b, strong, .bold
	font-weight 600
			
a
	transition $transition
	text-decoration none
	color $black
	&:hover, &:focus
		text-decoration none
		outline none
		color rgba($black, .7)


ul
	list-style square outside none

ol
	list-style decimal outside none
	
ul, ol
	padding 0 0 0 20px
	
ul, ol	
	&.list-none
		list-style none
		padding 0
		
b, strong
	font-weight 600

blockquote
	padding 25px
	color $black
	font-family $f2
	font-size 18px
	line-height 1.5em
	margin-top 30px
	letter-spacing 0
	border-left 5px solid $lgray
	
cite
	font-size 10px
	display block
	font-weight 700
	color $black
	line-height 1.5em
	text-transform uppercase
	letter-spacing 0.25em
	font-family $f1
	font-style normal
	margin-top 20px

pre
	overflow auto
	clear both
	position relative
	max-width 100%
	margin 0 0 20px 0
	background-color $llgray
	padding 20px 40px 20px 20px
	font-family monospace
	white-space pre-wrap
	color inherit
	&::after
		content '\f121'
		position absolute
		padding 2px 0 0 0
		width 20px
		top 0
		right 0
		height 100%
		font-size 10px
		background-color $black
		color $white
		font-family 'FontAwesome'
		text-align center

dl
	margin-bottom 20px
	
dt
	font-weight 600
	color $black
	text-transform uppercase
	font-size 14px

dd
	margin-bottom 7px

table
	width 100%
	max-width 100%
	margin 0 0 20px 0
	
th, caption
	background-color $black
	color $white
	text-transform uppercase
	letter-spacing 1px
	font-weight 400

th, td
	padding 10px 15px
	font-size 13px

tr:nth-child(odd)
	background-color $llgray

.alert
	border-radius 0
	text-transform uppercase
	font-size 13px
	letter-spacing 1px

.text-center
	text-align center

.cwhite
	color $lwhite
		
placeholder()
	&::-webkit-input-placeholder 
		font-size 13px
		color #aaaaaa
		letter-spacing 1px
	&:-moz-placeholder  /* Firefox 18- */
		font-size 13px
		color #aaaaaa
		letter-spacing 1px
	&::-moz-placeholder  /* Firefox 19+ */
		font-size 13px
		color #aaaaaa
		letter-spacing 1px
	&:-ms-input-placeholder  
		font-size 13px
		color #aaaaaa
		letter-spacing 1px
		
label
	display block
	font-size 13px
	font-weight 600
	color $black
	letter-spacing 1px
	line-height normal
	margin-bottom 6px
	span
		color $red
		font-size 15px
		
input, textarea
	placeholder()

input,
textarea,
select
	padding 0
	font-size 13px
	background-color transparent
	border none
	border-bottom 1px solid $gray
	border-radius 0
	width 100%
	display block
	box-shadow none
	outline none
	margin-top 10px
	margin-bottom 20px
	padding-bottom 10px
	letter-spacing 1px

select
	appearance none
	-webkit-appearance none
	-moz-appearance none
	background transparent url('../img/select_bg.png') no-repeat 100% 35%

input:focus,
textarea:focus,
select:focus,
button:focus
	outline none

textarea
	max-width 100%

.return-msg
	margin-bottom 0
	color $red

/*----------------------*/ 
/*   04-BUTTONS         */ 
/*----------------------*/

.btn
	display inline-block
	text-align center
	text-transform uppercase
	border-radius 0px
	box-shadow none
	letter-spacing 1px
	font-size 12px
	outline none
	transition $transition
	i
		margin-left 5px
	&.disable
		cursor default
	&:focus,
	&:active
		outline none

.btn-white
	border 1px solid $lwhite
	color $black
	background-color $lwhite
	&:hover
		color $lwhite
		background-color transparent
		
.btn-black
	border 1px solid $lblack
	color $white
	background-color $black
	&:hover
		color $black
		background-color transparent
		
.btn-border-white
	border 1px solid $lwhite
	color $white
	background-color transparent
	&:hover
		color $black
		background-color $lwhite
		
.btn-border-black
	border 1px solid $lblack
	color $black
	background-color transparent
	&:hover
		color $lwhite
		background-color $black

.link
	font-weight 700
	letter-spacing 0.1em
	font-size 13px
	color $black
	text-transform uppercase
	&:hover
		opacity .8
	
.btn-reply
	background-color $lgray
	color $text
	&:hover
		color $black

// Size Btn

.btn-xs
	font-size 10px
	padding 5px 15px

.btn-sm
	padding 5px 20px

.btn-md
	padding 10px 35px
			
.btn-lg
	padding 15px 40px

// Animate Icon

@keyframes animateIconTop
	49%
		transform translateY(-100%)
	50%
		opacity 0
		transform translateY(100%)
	51%
		opacity 1

@keyframes animateIconBottom
	49%
		transform translateY(100%)
	50%
		opacity 0
		transform translateY(-100%)
	51%
		opacity 1

@keyframes animateIconLeft
	49%
		transform translateX(-100%)
	50%
		opacity 0
		transform translateX(100%)
	51%
		opacity 1
		
@keyframes animateIconRight
	49%
		transform translateX(100%)
	50%
		opacity 0
		transform translateX(-100%)
	51%
		opacity 1
		
.btn-animateLeft
	&:hover i
		animation animateIconLeft .4s forwards
		
.btn-animateRight
	&:hover i
		animation animateIconRight .4s forwards
		
.btn-animateTop
	&:hover i
		animation animateIconTop .4s forwards
		
.btn-animateBottom
	&:hover i
		animation animateIconBottom .4s forwards
	

/*----------------------*/ 
/*   05-SECTION         */ 
/*----------------------*/

main
	padding 40px 0
	
.jarallax,
.section
	position relative
	background-size cover
	background-position center center
	background-repeat no-repeat
	overflow hidden

.section
	padding 50px 0
	&::after
		content ''
		clear both
		display table

.section-gray
	background-color lighten($lgray, 75%)

.aligner
	display -webkit-box
	display -webkit-flex
	display -ms-flexbox
	display flex
	-webkit-box-align center
	-webkit-align-items center
	-ms-flex-align center
	align-items center
	-webkit-box-pack center
	-webkit-justify-content center
	-ms-flex-pack center
	justify-content center
	min-height 80vh
	padding 80px 0
	
.header-fixed .aligner
	min-height 95vh
	padding 170px 0 80px 0

.aligner-item
	-webkit-box-flex 0
	-webkit-flex none
	-ms-flex none
	flex none
	max-width 100%
	min-width 40%
	
/*----------------------*/ 
/*   06-SOCIAL LINKS    */ 
/*----------------------*/

.social-link
	margin 0 0 20px 0
	padding 0
	list-style-type none
	&::after
		content ''
		display table
		clear both
	li
		float left
		a
			display block
			height 22px
			width 22px
			border-radius 2px
			font-size 11px
			line-height 22px
			text-align center
			color $white
			background-color $black
	li + li
		margin-left 8px

/*----------------------*/ 
/*   07-TOOLTIP         */ 
/*----------------------*/

.vl-tooltip
	border-radius 1px
	border none
	background-color $black
	color rgba($lwhite, 95%)

/* Use this next selector to style things like font-size and line-height: */
.vl-tooltip .tooltipster-content
	font-family $f1
	font-size 11px
	line-height 1.8em
	text-transform uppercase
	letter-spacing 3px
	padding 3px 10px 3px 13px


/*----------------------*/ 
/*   08-CLIENTS         */ 
/*----------------------*/

.logo-wrap
	position relative
	text-align center
	margin-bottom 20px

/*----------------------*/ 
/*   09-SERVICE BOX     */ 
/*----------------------*/

.service-box
	margin-bottom 20px
	.icon-holder
		float left
		margin-right 15px
		margin-bottom 15px
		text-align center
		padding 10px
		min-width 45px
		i
			font-size 24px
			color $black
		h5
			margin-bottom 10px
	.content-holder
		padding-left 60px
	
/*----------------------*/ 
/*   10-COUNTERUP       */ 
/*----------------------*/

.counter-item
	margin-bottom 20px
	.end-value
		margin-bottom 5px
		font-size 46px
		color $black
	i
		font-style normal

/*----------------------*/ 
/*   11-TABS            */ 
/*----------------------*/

.nav-tabs
	border-bottom none
	
.nav-tabs .nav-link
	margin-bottom 15px
	padding 15px 20px
	font-size 13px
	letter-spacing 1px
	text-transform uppercase
	border-radius 0
	border none
	background-color $lgray
	position relative
	color $text
	&:focus
		background-color $lgray
	&:not(.active):hover
		color $black
	&.active, &.active:hover, &.active:focus
		background-color $black
		color $lwhite
		&::after
			content ''
			position absolute
			bottom -6px
			left 0
			width 0
			height 0
			border-left 12px solid $lblack
			border-bottom 6px solid transparent
			animation tabCur .4s forwards

.tab-content .tab-pane
	padding 0 15px 20px 15px

@keyframes tabCur
	0%
		transform translateY(-100%)
		opacity 0
	100%
		transform translateY(0)
		opacity 1

/*----------------------*/ 
/*   12-ACCORDION       */ 
/*----------------------*/

.panel-title a, .panel-title a:focus
	position relative
	display block
	padding 15px 65px 15px 20px
	margin-bottom 15px
	font-size 13px
	text-transform uppercase
	letter-spacing 1px
	background-color $lgray
	transition $transition
	color $text
	&::before
		position absolute
		top 0
		right 0
		bottom 0
		width 55px
		border-left 2px solid $white
		font-size 18px
		line-height 55px
		text-align center
		background-color $lgray
		font-family 'FontAwesome'
		content '\f107'
		transition $transition

.panel-title a[aria-expanded="true"]:before
	content '\f106'
	background-color $black
	color $lwhite

.panel-title a:hover
	color $black
	 
.panel-title a[aria-expanded="true"]
	background-color $black
	color $lwhite
	
.panel-group .panel-heading + .panel-collapse > .list-group, .panel-group .panel-heading + .panel-collapse > .panel-body
	padding 0 15px 20px 15px
	

/*----------------------*/ 
/*   13-PROGRESS BAR    */ 
/*----------------------*/

.progress-bar
	position relative
	margin 0 0 20px 0
	h6
		width 40%
		float left
		
.progress-outer
	height 1px
	display inline-block
	vertical-align middle
	position relative
	width 60%
	background-color $transparent
	.progress-inner
		height 1px
		float left
		width 50px
		background-color $black
		
.progress-inner > span
	position relative
	float right
	left 40px
	top -14px
	color $black
	&::after
		content '%'
		margin-left 2px

@media (max-width: 768px)
	.progress-bar
		h6
			float none
			
/*----------------------*/ 
/*   14-PAGINATION      */ 
/*----------------------*/

.page-navigation
	display inline-block
	position relative
	margin 0
	padding 0
	margin-top 90px
	padding-top 50px
	list-style-type none
	&::after
		content ''
		display block
		width 30px
		height 2px
		background-color $black
		position absolute
		top 0
		left 50%
		margin-left -15px
	li
		float left
		margin 0 3px
		min-width 40px
		min-height 40px
		background-color $lgray
		a
			height 40px
			width 40px
			line-height 40px
			font-size 14px
			text-align center
			display block
			float left
			position relative
			color $text			
			i
				position relative
				z-index 2
			span
				position absolute
				z-index 0
				display inline-block
				text-transform uppercase
				text-align center
				background-color $black
				height 100%
				right 0
				letter-spacing 1.5px
				width (70px+40px)
				padding-right 40px
				line-height 40px
				margin-left 0
				transition transform .4s ease
				transform scaleX(0)
				transform-origin 100% 50%
			&.nav-right
				span
					left 0
					padding-left 40px
					padding-right 0
					transform-origin 0 50%
			&:hover, &.active
				color $white
				span
					transform scaleX(1)


/*----------------------*/ 
/*   15-VL_SLIDER       */ 
/*----------------------*/

dots()
	height 4px
	width 4px
	display block
	border none
	border-radius 50%
	box-shadow 0 1px 1px 0px rgba($lblack, .1)
	background-color rgba($lblack, .2)
	
.flexslider ul
	margin 0
	padding 0
	list-style-type none
	&::after
		display table
		clear both
		content ''		

.flex-prev,
.flex-next
	position absolute
	top 50%
	margin-top -20px
	opacity 0
	z-index 55
	height 40px
	width 40px
	background-color $lwhite
	color $black
	font-size 14px
	text-align center
	line-height 40px
	&:hover
		opacity 1
	&:active
		opacity .7

.flex-prev
	left -10px
	&::after
		content '\f104'
		font-family 'FontAwesome'
.flex-next
	right -10px
	&::after
		content '\f105'
		font-family 'FontAwesome'

.flexslider
	overflow hidden
	&:hover	
		.flex-prev,
		.flex-next
			opacity .9
		.flex-prev
			left 0
		.flex-next
			right 0

.image-slider-wrap
	overflow hidden
	position relative
	text-align center
	margin-bottom 20px
	.blind-01, 
	.blind-02
		position absolute
		top 0
		left 0
		height 100%
		width 100%
		transform translateX(-100%)
	.blind-01
		background-color $lgray
		z-index 40
	.blind-02
		background-color $black
		z-index 45	
	.flex-control-nav
		display inline-block
		list-style-type none
		padding 0
		margin 0
		li
			float left
			margin 0 6px
			a
				dots()
				text-indent 9999px
				&.flex-active
					background-color rgba($lblack,.9)

/*----------------------*/ 
/*   16-VL_LOGO         */ 
/*----------------------*/

.logo
	font-size 18px
	line-height 1em
	text-transform uppercase
	color $black
	letter-spacing 4px
	> span
		position relative
		display inline-block
		transition $transition
		transform-origin 50% 100%
	&:hover
		color $black
		> span
			transform rotateY(180deg)

/*----------------------*/ 
/*   17-VL_FOOTER       */ 
/*----------------------*/

.content
	z-index 1
	position relative
	background-color $white

footer
	width 100%

.footer
	background-color $lgray
	width 100%
	padding 60px 0

p.copy
	margin-top 30px
	color #777
	span
		color $black

.footer-menu, 
.footer-social
	list-style-type none
	margin 0
	padding 0
	display inline-block
	margin-top 30px
	li
		display inline-block
		margin 0 8px
		a
			text-transform uppercase
			font-size 13px
			letter-spacing 1px
			color rgba($black, .7)
			&:hover
				color $black

@media (max-width: 767px)
	footer
		position relative !important

/*----------------------*/ 
/*   18-PAGE-TITLE      */ 
/*----------------------*/

.page-title
	padding 75px
	text-align center
	background-color rgba($lwhite, 93%)
	h1
		font-size 31px
		line-height 1.4em
		letter-spacing 4px
		padding-left 4px
	p
		font-size 13px
		margin-top 12px
		text-transform uppercase
		letter-spacing 0.2em
		margin-bottom 0

@media (max-width:768px)
	.page-title
		padding 55px
		h1
			font-size 28px
			
@media (max-width:480px)
	.page-title
		padding 40px
		h1
			font-size 22px


/*----------------------*/ 
/*   19-VL_MENU         */ 
/*----------------------*/
	
.header-fixed .header-wrap
	position fixed !important

.header-wrap.no-transparent
	background-color $white
	
.header-fixed .header-wrap.no-transparent
	background-color $white
	
.header-wrap
	display table
	top 0
	left 0
	width 100%
	height 90px
	z-index 555
	transition transform 0.35s, opacity 0.35s, background-color 0.35s
	.header-container
		display table-cell
		vertical-align middle

.fixedheader.scroll-down .header-wrap
	transform translateY(-85%)
	opacity 0

.fixedheader.scroll-up:not(.fullscreen-nav-open) .header-wrap:not(.no-transparent)
	background-color $white

// Fix Menu bg
.fullscreen-nav-open 
	.header-wrap
		background-color transparent !important
	.logo
		color $white	

body:not(.header-fixed) .fullscreen-menu-toggle
	z-index 1001

.fullscreen-menu-toggle	
	position relative
	color $black
	font-size 14px
	letter-spacing 3px
	text-transform uppercase
	> span
		position relative
		line-height 1em
		opacity 0
		left -10px
		transition all .2s ease-in-out
	> .menu-bars
		margin-left 8px
		
.menu-bars
	position relative
	display inline-block
	width 1em
	height 1em
	padding 0
	margin-top -1px
	vertical-align middle
	> i
		position absolute
		left 0
		width 1em
		height 2px
		display inline-block
		transition all .4s ease-in-out
		background-color $black
		&:first-child
			top 0
		&:nth-child(2)
			top 50%
			margin-top -1px
		&:last-child
			top 100%
			margin-top -2px
	&.active
		> i:first-child
			transform rotate(-135deg)
		> i:nth-child(2)
			opacity 0			
		> i:last-child
			transform rotate(135deg)
		> i:first-child,
		> i:last-child
			top 0
			margin-top .333333em

.fullscreen-nav-open
	.fullscreen-menu-toggle
		> span
			color $lwhite
			transition-delay .4s
		> .menu-bars i
			background-color $lwhite

.fullscreen-nav-open .fullscreen-menu-toggle > span, 
.fullscreen-menu-toggle:hover > span
	opacity 1
	left 0 

#fullscreen-navigation
	position fixed
	top 0
	left 0
	width 100%
	height 100%
	display none
	z-index 444
	background-color rgba(#000, 90%)
	> .inner-wrap
		position absolute
		top 50%
		left 0
		width 100%
		max-height 100%
		overflow-y auto
		text-align center
		padding 50px 0
		transform translateY(-50%)


#fullscreen-navigation > .inner-wrap > nav > .nav_menu,
#fullscreen-navigation > .inner-wrap > nav > .nav_menu ul
	margin 0
	padding 0 0 5px 0
	list-style-type none

#fullscreen-navigation .nav_menu 
	a
		display inline-block
		padding 4px 0
		font-size 14px
		text-transform uppercase
		letter-spacing 1px
		color #777
		&:hover
			color $lwhite

#fullscreen-navigation .nav_menu .sub-menu
	display none
	a
		text-transform capitalize
		padding 2px 0
		font-size 12px
		letter-spacing 1px

#fullscreen-navigation .nav_menu li.menu-item-has-children
	> a::after
		content '\f107'
		font-family 'FontAwesome'
		margin-left 4px
	> a.sub-active
		color $lwhite
		&::after
			content '\f106'

#fullscreen-navigation li.current-menu-item > a,
#fullscreen-navigation li.current-page-item > a
	color $lwhite

#fullscreen-navigation .socials
	margin-top 25px
	a
		display inline-block
		height 35px
		width 35px
		text-align center
		line-height 35px
		white-space nowrap
		color #777
		&:hover
			color $white

/*----------------------*/ 
/*   20-WIDGETS         */ 
/*----------------------*/

.sidebar
	padding 0 20px
	
@media (max-width: 768px)
	.sidebar
		padding 0
		margin-top 50px

.widget
	margin-bottom 50px
	width 100%
	clear both
	position relative
	&:last-child
		margin-bottom 0

.widget-title
	text-transform uppercase
	margin-bottom 20px
	font-size 14px
	letter-spacing 3px
	line-height 3em
	font-weight 600
	position relative
	&::after
		content ''
		position absolute
